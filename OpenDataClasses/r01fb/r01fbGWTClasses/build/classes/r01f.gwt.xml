<?xml version="1.0" encoding="UTF-8"?>
<!--
In order to use this library a 
	<inherits name='r01f'/>
line MUST be added in the gwt.xml where the library is to be used 
-->
<module>
	<inherits name='com.google.gwt.user.User' />
	<source path="r01f"
			excludes="r01f/persistence">
	</source>
</module> 